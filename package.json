{
  "name": "@funda/storybook-wrapper-helper",
  "description": "storybook warpper helper",
  "version": "1.0.0",
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "github:funda-frontend/storybook-wrapper-helper",
  "license": "MIT",
  "main": "./dist/storybook-wrapper.umd.js",
  "module": "./dist/storybook-wrapper.es.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/storybook-wrapper.es.js",
      "require": "./dist/storybook-wrapper.umd.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "prepublishOnly": "npm run build",
    "link-local": "npm run build && npm link",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@funda/eslint-config": "^1.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^8.0.3",
    "@semantic-release/release-notes-generator": "^10.0.2",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "semantic-release": "^18.0.0",
    "vite": "^2.6.13",
    "vite-plugin-dts": "^0.9.3"
  },
  "engines": {
    "node": "^14.17.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json}": "eslint --fix"
  }
}
